{
  "name": "qqd",
  "version": "1.4.2",
  "description": "Quick-and-dirty debugging output for tired programmers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "precommit": "npm run prettier:fix && npm test",
    "prepare": "husky install",
    "prettier:fix": "prettier --write --no-config '*.json' 'src/**/*.{ts,json}'",
    "release": "np",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsquest/q.js.git"
  },
  "keywords": [
    "dirty",
    "debug",
    "q",
    "js",
    "javascript",
    "typescript"
  ],
  "author": "Thomas Queste <tom@tomsquest.com> (http://www.tomsquest.com/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tomsquest/q.js/issues"
  },
  "homepage": "https://github.com/tomsquest/q.js#readme",
  "jest": {
    "notify": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 60000,
    "rootDir": "src"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "np": "7.5.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  }
}
